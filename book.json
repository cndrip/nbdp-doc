        "theme-default-customize",
        "expandable-chapters-small"
    ],
    "language" : "zh-hans",
    "styles": {
        "website": "./styles/website.css"
    },
    "pluginsConfig": {
        "github-issue-feedback-language-custom": {
            "repo": "cndrip/nbdp-doc",
